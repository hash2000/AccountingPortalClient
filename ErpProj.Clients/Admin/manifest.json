{
  "_version": "1.12.0",
  "sap.app": {
    "id": "ErpProj.Home",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ErpProj.Home.Views.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {},
        "sap.ui.commons": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ErpProj.Home.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "ErpProj/Modules",
        "controlId": "NavigationPagesContainer",
        "controlAggregation": "pages"
      },

      "routes": [
        {
          "pattern": "",
          "name": "Overview",
          "target": "Overview"
        },
        {
          "pattern": "MenuItems",
          "name": "MenuItems",
          "target": "MenuItems"
        },
        {
          "pattern": "GroupsAndRoles",
          "name": "GroupsAndRoles",
          "target": "GroupsAndRoles"
        },
        {
          "pattern": "Profiles",
          "name": "Profiles",
          "target": "Profiles"
        }
      ],

      "targets": {
        "Overview": {
          "viewId": "Overview",
          "viewName": "Overview"
        },
        "MenuItems": {
          "viewId": "MenuItems",
          "viewName": "MenuItems.MenuItems"
        },
        "GroupsAndRoles": {
          "viewId": "GroupsAndRoles",
          "viewName": "GroupsAndRoles.GroupsAndRoles"
        },
        "Profiles": {
          "viewId": "Profiles",
          "viewName": "Profiles.Profiles"
        }
      }
    }
  }
}
